{
  "name": "find-the-capital",
  "version": "1.0.0",
  "description": "sample app for https://restcountries.eu/ API",
  "main": "index.js",
  "scripts": {
    "dev:start": "node_modules/.bin/babel-node src/index.js",
    "test:dev": "rm -rf test-results && mkdir test-results && mocha --config configs/mocha.dev.js",
    "lint:all": "eslint \"src/**/*.js\"",
    "prettier:staged": "pretty-quick --staged",
    "prettier:check": "prettier --check \"src/**/*.js\"",
    "prettier:fix": "prettier \"src/**/*.js\"",
    "lint:staged": "lint-staged",
    "verify-staged": "npm run lint:staged && npm run prettier:staged"
  },
  "author": "Oleg Konyk (oleg.konyk@aol.com)",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/register": "^7.7.7",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.9.0",
    "husky": "^4.0.3",
    "lint-staged": "^9.5.0",
    "mocha": "^7.0.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^4.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "winston": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run verify-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "inquirer": "^7.0.3"
  }
}
